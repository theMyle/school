Decimal to Hex:
1. 357
	357/16 = 22	r = 5
	22/16 = 1	r = 6
	1/16 = 0	r = 1
	answer = 165 hex

2. 788
	788/16 = 49	r = 4
	49/16 = 3	r = 1
	3/16 = 0	r = 3
	answer = 314 hex

Hex to Decimal:
3. FA8
	8 * 16^0 = 8
	10 * 16^1 = 160
	15 * 16^2 = 3840
	answer = 4008 decimal

4. 92B
	11 * 16^0 = 11
	2 * 16^1 = 32
	9 * 16^2 = 2304
	answer = 2347 decimal

Octal to Hex
5. 521
	1 = 001
	2 = 010
	5 = 101
	101010001 = 0001 0101 0001
		    1	 5    1
	answer = 151 hex

6. 725 
	5 = 101
	2 = 010
	7 = 111
	111 010 101 = 0001 1101 0101
			1    D    5
	answer = 1D5 hex

Hex to Octal
7. B3
	3 = 0011
	B = 1011
	  = 010 110 011
	answer = 263

8. CD0
	0 = 0000
	D = 1101
	C = 1100
	  = 110 011 010 000
	answer = 6320

Binary to Hex
9. 11 0101 0101 0111 0011
	= 0011 0101 0101 0111 0011
	answer = 35573 hex

10. EB8
	8 = 1000
	B = 1011
	E = 1110
	answer = 1110 1011 1000





